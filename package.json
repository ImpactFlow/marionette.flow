{
  "name": "marionette.flow",
  "version": "0.1.1",
  "description": "Build user workflows with views and directed graphs",
  "main": "marionette.flow.js",
  "scripts": {
    "build": "node_modules/.bin/webpack",
    "lint": "./node_modules/.bin/eslint -c .eslintrc.json .",
    "test": "node_modules/.bin/jest"
  },
  "keywords": [
    "marionette",
    "workflow",
    "view",
    "directed",
    "graph",
    "traversal"
  ],
  "contributors": [
    "Kevin Garsjo <krgarsjo@gmail.com> (https://www.kgarsjo.com)",
    "ImpactFlow <impactflow@impactflow.com> (https://www.impactflow.com)"
  ],
  "license": "MIT",
  "dependencies": {
    "flow": "ImpactFlow/flow#v0.3.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "backbone": "^1.0.0",
    "backbone.marionette": "3.1.0",
    "eslint": "^3.12.2",
    "jasmine": "^2.5.2",
    "jest": "^18.0.0",
    "jquery": "^2.2.0",
    "webpack": "^1.14.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/**/*.js",
      "!**/marionette.flow.js",
      "!**/coverage/**",
      "!**/dist/**",
      "!**/node_modules/**",
      "!**/tests/**",
      "!**/webpack.config.js"
    ],
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "verbose": true
  },
  "peerDependencies": {
    "backbone": ">=1.0.0",
    "backbone.marionette": ">=1.8.8"
  },
  "resolutions": {
    "underscore": "1.8.3",
    "backbone": "1.3.3",
    "debug": "2.2.0",
    "estraverse": "4.2.0",
    "strip-bom": "3.0.0",
    "strip-json-comments": "2.0.1",
    "supports-color": "3.2.3",
    "inherits": "2.0.3",
    "readable-stream": "2.2.2",
    "ms": "0.7.2",
    "acorn": "3.3.0",
    "once": "1.4.0",
    "string-width": "1.0.2",
    "esprima": "3.1.3",
    "minimist": "0.0.8",
    "wordwrap": "1.0.0",
    "callsites": "2.0.0",
    "interpret": "0.6.6",
    "resolve": "1.2.0",
    "yargs": "6.6.0",
    "async": "1.5.2",
    "source-map": "0.5.6",
    "camelcase": "3.0.0",
    "cliui": "3.2.0",
    "webidl-conversions": "4.0.0",
    "assert-plus": "0.2.0",
    "punycode": "1.4.1",
    "memory-fs": "0.3.0"
  }
}
